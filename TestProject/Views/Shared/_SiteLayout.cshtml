<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <title>@ViewBag.Identities.Title- @ViewBag.Identities.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="@Html.Raw(ViewBag.Identities.Description)" />
    <meta name="keywords" content="@Html.Raw(ViewBag.Identities.Keywords)" />
    <!-- css -->
    <link href="~/Content/Moderna/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Moderna/css/fancybox/jquery.fancybox.css" rel="stylesheet">
    <link href="~/Content/Moderna/css/jcarousel.css" rel="stylesheet" />
    <link href="~/Content/Moderna/css/flexslider.css" rel="stylesheet" />
    <link href="~/Content/Moderna/css/style.css" rel="stylesheet" />

    <link href="~/Content/Moderna/skins/default.css" rel="stylesheet" />


</head>



<body>
    <div id="wrapper">
        <!-- start header -->
        <header>
            <div class="navbar navbar-default navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/Home/Index/"><img src="@ViewBag.Identities.LogoURL" height="30" width="75"/>@ViewBag.Identities.Title </a>
                    </div>
                    <div class="navbar-collapse collapse ">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="/Home/Index/">Home</a></li>

                            <li><a href="/Home/Abouts/">About Us</a></li>
                            <li><a href="/Home/Services/">Our Services</a></li>
                            <li><a href="/Home/Blogs/">Blog</a></li>
                            <li><a href="/Home/Contacts/">Contact</a></li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle " data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false">My Profile <b class=" icon-angle-down"></b></a>
                                <ul class="dropdown-menu">

                                    @{
                                        if (Session["Role"] == null)
                                        {
                                            <li><a href="/Admins/Login/@Session["ID"]">Profile</a></li>
                                            <li><a href="/Admins/Logout">Logout</a></li>
                                        }

                                    }

                                    @{
                                        if (Session["Role"] != null)
                                        {
                                            <li><a href="/Admins/UserPageShowDetails/@Session["ID"]">Profile</a></li>
                                            <li><a href="/Admins/Logout">Logout</a></li>
                                        }

                                    }


                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
        <!-- end header -->
    @RenderBody()
     
        @{
            Html.RenderAction("FooterPartial", "Home");
        }
    </div>
    <a href="#" class="scrollup"><i class="fa fa-angle-up active"></i></a>
    <!-- javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="~/Content/Moderna/js/jquery.js"></script>
    <script src="~/Content/Moderna/js/jquery.easing.1.3.js"></script>
    <script src="~/Content/Moderna/js/bootstrap.min.js"></script>
    <script src="~/Content/Moderna/js/jquery.fancybox.pack.js"></script>
    <script src="~/Content/Moderna/js/jquery.fancybox-media.js"></script>
    <script src="~/Content/Moderna/js/google-code-prettify/prettify.js"></script>
    <script src="~/Content/Moderna/js/portfolio/jquery.quicksand.js"></script>
    <script src="~/Content/Moderna/js/portfolio/setting.js"></script>
    <script src="~/Content/Moderna/js/jquery.flexslider.js"></script>
    <script src="~/Content/Moderna/js/animate.js"></script>
    <script src="~/Content/Moderna/js/custom.js"></script>

</body>
</html>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li class="nav-item"><a class="nav-link" href="/Blogs/Create/"><i class="ti-control-record"></i>Create</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Comments/Index/"><i class="ti-control-record"></i>Comments</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Blogs/Index/"><i class="ti-control-record"></i>Blog List</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript: void(0);"><i class="ti-bar-chart"></i><span>About</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li class="nav-item"><a class="nav-link" href="/Abouts/Create/"><i class="ti-control-record"></i>Create</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Abouts/Index/"><i class="ti-control-record"></i>All About</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript: void(0);"><i class="ti-bar-chart"></i><span>Slider</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li class="nav-item"><a class="nav-link" href="/Sliders/Create/"><i class="ti-control-record"></i>Create</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Sliders/Index/"><i class="ti-control-record"></i>All Sliders</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript: void(0);"><i class="ti-bar-chart"></i><span>Services</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li class="nav-item"><a class="nav-link" href="/Services/Create/"><i class="ti-control-record"></i>Create</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Services/Index/"><i class="ti-control-record"></i>Service List</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript: void(0);"><i class="ti-bar-chart"></i><span>Identity</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li class="nav-item"><a class="nav-link" href="/Identities/Create/"><i class="ti-control-record"></i>Create</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Identities/Index/"><i class="ti-control-record"></i>Identity List</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="javascript: void(0);"><i class="ti-bar-chart"></i><span>Contact Informations</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li class="nav-item"><a class="nav-link" href="/Contacts/Create/"><i class="ti-control-record"></i>Create</a></li>
                            <li class="nav-item"><a class="nav-link" href="/Contacts/Index/"><i class="ti-control-record"></i>Contact Information List</a></li>
                        </ul>
                    </li>







                </ul>
            </div>

    

    <!-- end left-sidenav-->

    <div class="page-wrapper">
        <!-- Page Content-->
        <div class="page-content">

            <div class="container-fluid">
                <!-- Page-Title -->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                        </div><!--end page-title-box-->
                    </div><!--end col-->
                </div>
                <!-- end page title end breadcrumb -->
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="container-fluid">


                                @RenderBody()

                            </div>
                        </div>
                    </div> <!-- end col -->
                </div> <!-- end row -->


            </div><!-- container -->
        </div>

    </div>

    <footer class="footer text-center text-sm-left">
        &copy; 2020 Crovex <span class="text-muted d-none d-sm-inline-block float-right">Crafted with <i class="mdi mdi-heart text-danger"></i> by Mannatthemes</span>
    </footer><!--end footer-->
    <!-- end page-wrapper -->
    <!-- jQuery  -->

    <script src="~/Content/HTML/assets/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/HTML/assets/js/metismenu.min.js"></script>
    <script src="~/Content/HTML/assets/js/waves.js"></script>

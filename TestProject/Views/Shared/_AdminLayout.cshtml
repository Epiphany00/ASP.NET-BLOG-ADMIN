    <script src="~/Content/plugins/flot-chart/jquery.flot.drawSeries.js"></script>
    <script src="~/Content/plugins/flot-chart/jquery.flot.uiConstants.js"></script>
    <script src="~/Content/plugins/flot-chart/jquery.flot-dataType.js"></script>
